<a href="car/{{this._id}}" class="text-decoration-none">
    <div class="card car-card border border-white mt-3 mb-3" style="width: 202px;">
        <img src="{{this.icon}}" class="card-img-top" alt="{{this.name}}'s image">
        <div class="card-body text-center">
            <h6 class="card-title text-white">{{this.name}}
                <span class="text-danger">{{this.likes}}<i class="bi bi-heart-fill text-danger"></i></span>
            </h6>
        </div>
    </div>
</a>